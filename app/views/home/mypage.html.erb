<div class="main posts-index">
  <div class="container">
    <p id="notice"><%= notice %></p>

    <h1>Participating Rooms</h1>

    <table>
      <thead>
        <tr>
          <th>Owner</th>
          <th>Participant</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @rooms.each do |room| %>
          <tr>
            <td><%= room.owner_id %></td>
            <td><%= room.participant_id %></td>
            <td><%= link_to 'Show', room %></td>
            <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'change profile', edit_user_registration_path %>
  <end>
<end>